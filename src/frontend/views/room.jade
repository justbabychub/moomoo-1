head
  title MooMoo
  #roomid(data!= roomid)
  link(rel="stylesheet" type="text/css" href="/css/style.css")
  script(src="https://cdn.socket.io/socket.io-1.3.5.js")
  script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
  script(src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js")
  script(src="/vendor/socket.io-stream.js")
  script(src="/vendor/soundmanager2-jsmin.js")
  script(src="/vendor/bar-ui.js")
html
  body
    #top-bar.top-bottom-bars
      #current-track-tags
      #progress-track
        hr#progress-track-bar
        #progress-track-ball

      button#invite-friends-button.top-bar-button Invite Friends
      input#file-upload(type="file")
      button#file-upload-input.top-bar-button(onclick="document.getElementById('file-upload').click();") Add Music

    #center-area
      #messages-window
      #track-queue
        #playlist
        #download-progress
        #users-upload-progress

      #message-form
      #username-form

      #userslist
      #music-player
        span#play-button
        span#pause-button
      #playlist-manip
        span#previous-track-button
        span#next-track-button
      #misc
        span#restart-button

    #bottom-bar.top-bottom-bars


    script(src="/js/moomoo-frontend.js")
